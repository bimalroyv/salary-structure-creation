import openpyxl
from win32com import client
path = input("Enter the path of the Excel file")
cell = input("Enter the cell address for ctc")
wb=openpyxl.load_workbook(path)
sh=wb['Sheet1']

ctc=input("enter the ctc")
wb[cell]=ctc
wb.save()

app = client.DispatchEx("Excel.Application")
app.Interactive = False
app.Visible = False
wb.ActveSheet.ExportAsFixedFormat(0,path)
wb.Close()

